{
    "$schema": "http://json.schemastore.org/eslintrc",
    "extends": [
        "prettier",
        "eslint-config-turbo"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ],
        "turbo/no-undeclared-env-vars": "off"
    },
    "parser": "@typescript-eslint/parser"
}